{
   "name": "trw",
   "version": "1.0.0",
   "description": "脚手架项目-用于快速搭建项目架构",
   "main": "dist/index.js",
   "bin": {
      "trw": "./dist/cli.js"
   },
   "scripts": {
      "build": "tsup",
      "start": "node dist/cli.js",
      "type-check": "tsc --noEmit",
      "format": "prettier --write .",
      "cspell": "cspell \"**/*.{ts,js,json,md}\"",
      "unlink:global": "pnpm remove -g trw; if ($LASTEXITCODE -eq 0 -or $LASTEXITCODE -eq $null) { Remove-Item $env:LOCALAPPDATA\\pnpm\\trw* -ErrorAction SilentlyContinue }"
   },
   "files": [
      "dist",
      "templates",
      "resources"
   ],
   "keywords": [
      "cli",
      "scaffolding",
      "generator"
   ],
   "author": "Turw",
   "license": "ISC",
   "packageManager": "pnpm@10.13.1",
   "dependencies": {
      "@clack/core": "^0.5.0",
      "@clack/prompts": "^0.11.0",
      "cac": "^6.7.14",
      "chalk": "^5.6.2",
      "cli-progress": "^3.12.0",
      "download-git-repo": "^3.0.2",
      "execa": "^9.6.0",
      "fast-glob": "^3.3.3",
      "fs-extra": "^11.3.2",
      "ora": "^9.0.0",
      "semver": "^7.7.3",
      "update-notifier": "^7.3.1",
      "validate-npm-package-name": "^7.0.0"
   },
   "devDependencies": {
      "@types/cli-progress": "^3.11.6",
      "@types/fs-extra": "^11.0.4",
      "@types/node": "^24.9.2",
      "@types/semver": "^7.7.1",
      "@types/update-notifier": "^6.0.8",
      "cspell": "^9.4.0",
      "prettier": "^3.6.2",
      "tsup": "^8.5.0",
      "typescript": "^5.9.3"
   }
}
